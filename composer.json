{
  "name": "codexten/yii-app-storage",
  "description": "storage app yii 2.0 or higher",
  "type": "library",
  "require": {
    "trntv/yii2-file-kit": "@stable",
    "mihaildev/yii2-elfinder": "1.3.0",
    "codexten/yii-web": "~2.0",
    "trntv/yii2-glide": "^1.0",
    "creocoder/yii2-flysystem": "0.8.*",
    "league/flysystem-aws-s3-v3": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\storage\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "hidev": "src/hidev/config.php",
      "params": "config/params.php",
      "core": "config/core.php",
      "storage": [
        "$core",
        "config/storage.php"
      ]
    }
  }
}
